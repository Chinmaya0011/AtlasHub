{
    "version": 2,
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/countries",
        "dest": "/api/countries.js"
      },
      {
        "src": "/api/countries/:id",
        "dest": "/api/countries.js"
      }
    ]
  }
  